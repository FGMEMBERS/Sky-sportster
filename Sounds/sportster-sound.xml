<?xml version="1.0" encoding="ISO-8859-1"?>

<!--from the bocian by AJ MacLeod--> 
<PropertyList>
  
  <fx>
    
    <crank>
      <name>engstart</name>
      <path>Sounds/engstart.wav</path>
      <property>/engines/engine[0]/cranking</property>
      <volume>
        <offset>0.4</offset>
      </volume>
    </crank>
    
    <crank>
      <name>crank</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <property>/engines/engine[0]/cranking</property>
        <not>
          <property>/engines/engine[0]/running</property>
        </not>
      </condition>
      <volume>
        <offset>0.5</offset>
      </volume>
    </crank>
    
    <crank>
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <offset>0.3</offset>
      </volume>
    </crank>
    
    <engine>
      <name>engine</name>
      <mode>looped</mode>
      <path>Sounds/wasp.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <property>/engines/engine[0]/mp-osi</property>
        <factor>0.00375</factor>
        <min>0.15</min>
        <max>0.5</max>
      </volume>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume>
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.7</factor>
      </volume>
      <pitch>
        <property>/engines/engine[0]/rpm</property>
        <factor>0.0004</factor>
        <min>0.5</min>
        <max>2.0</max>
        <offset>0.15</offset>
      </pitch>
    </engine>
    
    <propeller>
      <name>propeller</name>
      <mode>looped</mode>
      <path>Sounds/turboprop1.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.2</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/sim/current-view/internal</property>
        <factor>0.5</factor>
      </volume>
      <pitch>
        <property>/engines/engine[0]/mp-osi</property>
        <factor>0.0036</factor>
        <min>0.5</min>
        <max>2.0</max>
        <offset>0.9</offset>
      </pitch>
    </propeller>
    
    <tyre-rumble>
      <name>tyre-rumble</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21-MI/Sounds/ground_roll3.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>velocities/groundspeed-kt</property>
        <factor>0.05</factor>
      </volume>
    </tyre-rumble>
    
    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
        <property>position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <min>0.03</min>
        <max>0.5</max>
      </volume>
      <pitch>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>
    
    <variometer-up>
      <name>variometer-up</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21-MI/Models/Interior/Panel/Instruments/vario/vario.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>instrumentation/ilec-sc7/audio</property>
            <value>0</value>
          </greater-than>
          <greater-than>
            <property>instrumentation/ilec-sc7/te-reading-mps</property>
            <value>0.01</value>
          </greater-than>
        </and>
      </condition>
      <pitch>
        <property>instrumentation/ilec-sc7/te-reading-mps</property> 
        <factor>0.246</factor>
        <offset>1.0</offset>
      </pitch>
      <volume>
        <property>instrumentation/ilec-sc7/volume</property>
        <max>1</max>
      </volume>
    </variometer-up>
    
    <variometer-down>
      <name>variometer-down</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21-MI/Models/Interior/Panel/Instruments/vario/vario2.wav</path>
      <condition>
        <and>
          <equals>
            <property>instrumentation/ilec-sc7/audio</property>
            <value>2</value>
          </equals>
          <greater-than>
            <property>instrumentation/ilec-sc7/te-reading-mps</property>
            <value>-10.66</value>
          </greater-than>
          <less-than>
            <property>instrumentation/ilec-sc7/te-reading-mps</property>
            <value>-0.01</value>
          </less-than>
        </and>
      </condition>
      <pitch>
        <property>instrumentation/ilec-sc7/te-reading-neg</property>
        <factor>-0.05</factor>
        <offset>1.0</offset>
      </pitch>
      <volume>
        <property>instrumentation/ilec-sc7/volume</property>
        <max>1</max>
      </volume>  
    </variometer-down>
    
    <winch_tow>
      <name>winch_tow</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21-MI/Sounds/winch_tow.wav</path>
      <condition>
        <not>
          <property>sim/hitches/winch/open</property>
        </not>
      </condition>
      <volume>
        <property>velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <max>1</max>
      </volume>
    </winch_tow>
    
    <whoosh-positive>
      <name>whoosh-positive</name>
      <mode>once</mode>
      <path>Aircraft/ASK21-MI/Sounds/whoosh.wav</path>
      <condition>
        <greater-than>
          <property>orientation/pitch-rate-degps</property>
          <value>15</value>
        </greater-than>
      </condition>
      <volume>
        <property>orientation/pitch-rate-degps</property>
        <factor>0.01</factor>
        <max>1</max>
      </volume>
    </whoosh-positive>
    
    <whoosh-negative>
      <name>whoosh-negative</name>
      <mode>once</mode>
      <path>Aircraft/ASK21-MI/Sounds/whoosh.wav</path>
      <condition>
        <less-than>
          <property>orientation/pitch-rate-degps</property>
          <value>-15</value>
        </less-than>
      </condition>
      <volume>
        <property>orientation/pitch-rate-degps</property>
        <factor>-0.01</factor>
        <max>1</max>
      </volume>
    </whoosh-negative>
    
    <cable_release>
      <name>cable_release</name>
      <mode>once</mode>
      <path>Aircraft/ASK21-MI/Sounds/winch_release.wav</path>
      <condition>
        <property>sim/hitches/winch/open</property>
      </condition>
    </cable_release>
    
    <click> 
      <name>Magnetos-off</name>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>controls/engines/engine[0]/magnetos</property>
        </not>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>
    
    <click> 
      <name>Magnetos-R</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine[0]/magnetos</property>
          <value>1</value>
        </equals>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>
    
    <click> 
      <name>Magnetos-L</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine[0]/magnetos</property>
          <value>2</value>
        </equals>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>
    
    <click> 
      <name>Magnetos-B</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine[0]/magnetos</property>
          <value>3</value>
        </equals>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>
    
  </fx>
  
</PropertyList>

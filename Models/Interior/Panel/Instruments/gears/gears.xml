<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>gears.ac</path>
  
  <animation>
    <object-name>support</object-name>
    <object-name>lightL</object-name>
    <object-name>lightC</object-name>
    <object-name>lightR</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>lightC</object-name>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>/gear/gear[0]/position-norm</factor-prop>
      <red>   0 </red>
      <green> 2 </green>
      <blue>  0 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>lightL</object-name>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>/gear/gear[1]/position-norm</factor-prop>
      <red>   0 </red>
      <green> 2 </green>
      <blue>  0 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>lightR</object-name>
    <condition>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>/gear/gear[2]/position-norm</factor-prop>
      <red>   0 </red>
      <green> 2 </green>
      <blue>  0 </blue>
    </emission>
  </animation>
  
</PropertyList>
